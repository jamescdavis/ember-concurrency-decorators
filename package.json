    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.3",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "~3.9.5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
